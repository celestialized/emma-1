<body class="<%= body_css_class %>">
  <div aria-describedby="cookieconsent:desc"
      aria-label="cookieconsent"
      aria-live="polite"
      class="cc-window cc-floating cc-type-info cc-theme-edgeless cc-bottom cc-left cc-color-override--1588655054 cc-invisible"
      role="dialog"
      style="display: none;">
  <!--googleoff: all-->
  <span class="cc-message"
        id="cookieconsent:desc">
        This website uses cookies to ensure you get the best experience on our website. 
        <a aria-label="learn more about cookies"
        class="cc-link"
        href="/privacy-policy/"
        rel="noopener noreferrer nofollow"
        role="button"
        tabindex="0"
        target="_blank">Learn more
      </a>
    </span>
    <div class="cc-compliance">
      <a aria-label="dismiss cookie message"
          class="cc-btn cc-dismiss"
          role="button"
          tabindex="0">Got it!</a>
    </div>
    <!--googleon: all-->
  </div>

  <div class="fb_reset"
       id="fb-root">
    <div style="position: absolute; top: -10000px; width: 0px; height: 0px;"></div>
  </div>
  <script>
    window.fbAsyncInit = function() {
      FB.init({
        xfbml: true,
        version: 'v4.0'
      });
    };

    (function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = 'https://connect.facebook.net/en_US/sdk/xfbml.customerchat.js';
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
  </script> 
  
